<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".umaractv.Activity_Umar50">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="225dp"
        android:layout_height="94dp"
        android:orientation="horizontal"
        android:layout_marginTop="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:importantForAccessibility="no"
            tools:ignore="ImageContrastCheck"
            tools:srcCompat="@tools:sample/avatars" />

        <LinearLayout
            android:layout_width="254dp"
            android:layout_height="93dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1"
                android:layout_width="184dp"
                android:layout_height="8dp"
                android:layout_weight="1"
                android:text="@string/kfc_pejetan"
                tools:ignore="NestedWeights" />

            <LinearLayout
                android:layout_width="122dp"
                android:layout_height="2dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                tools:ignore="TextSizeCheck">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:importantForAccessibility="no"
                    app:srcCompat="@android:drawable/ic_dialog_map"
                    tools:ignore="ImageContrastCheck" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/_1_2km" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/_7_9min" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="165dp"
                android:layout_height="24dp">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    android:importantForAccessibility="no"
                    app:srcCompat="@android:drawable/ic_dialog_map"
                    tools:ignore="ImageContrastCheck" />

                <TextView
                    android:layout_width="125dp"
                    android:layout_height="wrap_content"
                    android:text="@string/_4_3" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="221dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout1"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            android:layout_weight="1"
            android:importantForAccessibility="no"
            tools:srcCompat="@tools:sample/avatars" />

        <LinearLayout
            android:layout_width="254dp"
            android:layout_height="93dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="184dp"
                android:layout_height="8dp"
                android:layout_weight="1"
                android:text="@string/kfc_kemangs"
                tools:ignore="NestedWeights" />

            <LinearLayout
                android:layout_width="122dp"
                android:layout_height="2dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                tools:ignore="TextSizeCheck">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:srcCompat="@android:drawable/ic_dialog_map"
                    tools:ignore="ContentDescription,ImageContrastCheck,ImageContrastCheck" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/_1_2km" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/_7_9min" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="165dp"
                android:layout_height="24dp">

                <ImageView
                    android:id="@+id/imageView6"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    android:importantForAccessibility="no"
                    app:srcCompat="@android:drawable/ic_dialog_map"
                    tools:ignore="ImageContrastCheck,InefficientWeight" />

                <TextView
                    android:layout_width="125dp"
                    android:layout_height="wrap_content"
                    android:text="@string/_4_3" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>


    <Button
        android:id="@+id/btnUmar50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Move"
        android:layout_marginTop="50dp"
        android:backgroundTint="@color/PrimaryAppColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6" />

</androidx.constraintlayout.widget.ConstraintLayout>

